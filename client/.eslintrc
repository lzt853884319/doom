{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "extends": "airbnb",
    "rules": {
        "semi": [0],
        "react/jsx-filename-extension": [0],
        "react/jsx-one-expression-per-line": [0],
        "no-console": [0],
        "no-plusplus": [0],
        "react/prop-types": [0],
        "react/no-typos": [0],
        "react/destructuring-assignment": [0],
        "react/require-default-props": [0]
    }
}
